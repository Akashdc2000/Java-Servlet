<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

  <display-name>Archetype Created Web Application</display-name>

<!--  1 -->
  <servlet>
    <servlet-name>httpServlet</servlet-name>
    <servlet-class>com.akash.HttpServletDemo</servlet-class>
  </servlet>

<!--  2 -->
  <servlet>
    <servlet-name>Servlet</servlet-name>
    <servlet-class>com.akash.ServletDemo</servlet-class>
  </servlet>

<!--  3 -->
  <servlet>
    <servlet-name>GenericServlet</servlet-name>
    <servlet-class>com.akash.GenericServletDemo</servlet-class>
  </servlet>

  <!--  1 -->
  <servlet-mapping>
    <servlet-name>httpServlet</servlet-name>
    <url-pattern>/httpservlet</url-pattern>
  </servlet-mapping>

  <!--  2 -->
  <servlet-mapping>
    <servlet-name>Servlet</servlet-name>
    <url-pattern>/servlet</url-pattern>
  </servlet-mapping>

  <!--  3 -->
  <servlet-mapping>
    <servlet-name>GenericServlet</servlet-name>
    <url-pattern>/genericservlet</url-pattern>
  </servlet-mapping>

</web-app>
